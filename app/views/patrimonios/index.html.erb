<%= flash[:notice] %>

<h1>Loaded Patrimonios</h1>

<table>
  <thead>
    <tr>
      <th>Sequencial Candidato</th>
      <th>Valor do Bem</th>
      <th>Descrição do Bem</th>
    </tr>
  </thead>
  <tbody>
    <% @patrimonios.each do |patrimonio| %>
      <tr>
        <td><%= patrimonio.sequencial_candidato %></td>
        <td><%= patrimonio.valor_bem %></td>
        <td><%= patrimonio.detalhe_bem %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @patrimonios %>
